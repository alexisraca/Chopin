<%= link_to edit_admin_product_path(variant.product), class: "btn btn-primary", data: { toggle: "tooltip" }, title: "Editar" do %>
  <%= fa_icon "pencil-square-o" %>
<% end %>

<%= link_to admin_product_variant_path(variant.product, variant), method: :delete, class: "btn btn-danger", data: { toggle: "tooltip", confirm: delete_variant_warning_text(variant) }, title: "Borrar", remote: :true do %>
  <%= fa_icon "trash-o" %>
<% end %>
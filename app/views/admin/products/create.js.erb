<% if !@product.new_record? %>
  window.history.pushState("", "", '<%= edit_admin_product_path(@product) %>');
<% end %>
$(".form-container").html("<%= j render 'form' %>");
window.bindDatePicker($(".datepicker"))
<% unless flash[:notice].blank? %>
  $.notify({ message: "<%= flash[:notice] %>" },{ type: 'success', placement: { align: "center" } });
<% end %>
<%= link_to edit_admin_user_path(user), class: "btn btn-primary", data: { toggle: "tooltip" }, title: "Editar", remote: :true do %>
  <%= fa_icon "pencil-square-o" %>
<% end %>

<% unless current_user == user %>
  <%= link_to admin_user_path(user), method: :delete, class: "btn btn-danger", data: { toggle: "tooltip" }, title: "Borrar", remote: :true do %>
    <%= fa_icon "trash-o" %>
  <% end %>
<% end %>
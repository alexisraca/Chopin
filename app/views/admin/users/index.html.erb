<%= link_to "Nuevo Usuario", new_admin_user_path, class: "btn btn-primary" %>

<table class="table table-bordered">
  <thead>
    <tr> 
      <th>Usuario
      </th>
      <th>Acciones
      </th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
  	<tr>
  		<td>
  			<%= user.username %>
  		</td>
  		<td>
  			<%= link_to "Editar", edit_admin_user_path(user), class: "btn btn-primary" %>
  		</td>
  	</tr>
  	<% end %>
  </tbody>
</table>
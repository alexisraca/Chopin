<%= form_for @order, url: admin_orders_path do |f| %>

  <%= render partial: "/shared/form_errors", locals: { instance: @order } %>

  <%= f.label :user_id %>
  <%= f.text_field :user_id %><br />

  <%= f.label :comment %>
  <%= f.text_field :comment %><br />

  <%= f.label :completed_at %>
  <%= f.text_field :completed_at %><br />

  <%= f.label :total %>
  <%= f.text_field :total %><br />

  <%= f.label :state %>
  <%= f.text_field :state %><br />

  <%= f.submit %>
<% end %> 
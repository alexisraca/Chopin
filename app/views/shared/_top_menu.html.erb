<div class="row top-menu-wrapper">
  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
     <div class="row">
       <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8">
       </div>
       <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
         <ul class="list-inline pull-right top-menu-block">
           <li class="text-center top-menu-block-element">
             <%= link_to "#", class: "display-block top-menu-link" do %>
               <i class="fa fa-bell-o">
                <span class="messages-count badge">0</span>
               </i>
             <% end %>
           </li>
           <li class="text-center top-menu-block-element">
             <%= link_to "#", class: "display-block top-menu-link" do %>
               <i class="fa fa-envelope-o">
                <span class="messages-count badge">0</span>
               </i>
             <% end %>
           </li>


           <li class="dropdown top-menu-block-element">
             <%= link_to "#", class: "display-block text-center top-menu-link dropdown-toggle", "data-toggle" => "dropdown", "aria-haspopup" => "true", "aria-expanded" => "true", id: "dropdownMenu1" do %>
               <i class="fa fa-cog">
                 <i class="fa fa-caret-down"></i>
               </i>
             <% end %>
             <ul class="dropdown-menu dropdown-menu-right" aria-labelledby="dLabel"  role="menu">
               <li>
                 <%= link_to destroy_user_session_path, class: "text-right" do %>
                   Salir
                   <i class="fa fa-sign-out"></i>
                 <% end %>
               </li>
             </ul>
           </li>
         </ul>
       </div>
     </div>
  </div>
</div>